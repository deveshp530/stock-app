<div class="row">
    <div class="col-5">
        <table class="table table-bordered table-dark table-hover">
        <thead>
            <tr>
            <th scope="col">Ticker</th>
            <th scope="col">Name</th>
            <th scope="col">Price</th>
            <th scope="col">Actions</th>
            </tr>
        </thead>
        <tbody>
            <% @tracked_stock.each do |stock| %>
                <tr>
                <th scope="row"><%= stock.ticker %></th>
                <td><%= stock.name %></td>
                <td><%= stock.price %></td>
                <td><%= 'Acton' %></td>
                </tr>
            <% end %>
        </tbody>
        </table>
    </div>
</div>